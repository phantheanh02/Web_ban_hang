                <script>
    function autoScrollAudio(){
        var j = 0;
        var status = 0;
        var currentTime = Math.floor(Date.now() / 1000);
        var size = $(".block-schedule-list-audio").size();
        var distance = 0;

        for (var i = 0; i <= size; i++) {
            var startTime = $('#audio-'+i).attr('data-start');

            var id = $('#audio-'+i).attr('data-id');
            var endTime = $('#audio-'+i).attr('data-end');

            console.log('audio-' + i, $('#audio-'+i).outerHeight());

            if(startTime <= currentTime && currentTime < endTime){
                var title = $('#audio-'+i).attr('data-title');
                $('#audio-'+i).addClass('active');
                break;

            }else{
                $('#audio-'+i).removeClass('active');
            }

            distance += $('#audio-'+i).outerHeight() + 10;
        };

        console.log(distance);

        $('#block-schedule-list-audio').scrollTop(distance);

    }

    function autoScrollVideo(){
        var j = 0;
        var status = 0;
        var currentTime = Math.floor(Date.now() / 1000);
        var size = $(".block-schedule-list-video").size();
        var distance = 0;

        for (var i = 0; i <= size; i++) {
            var startTime = $('#schedule-'+i).attr('data-start');

            var id = $('#schedule-'+i).attr('data-id');
            var endTime = $('#schedule-'+i).attr('data-end');

            if(startTime <= currentTime && currentTime < endTime){
                var title = $('#schedule-'+i).attr('data-title');
                $('#schedule-'+i).addClass('active');
                break;

            }else{
                $('#schedule-'+i).removeClass('active');
            }

            distance += $('#schedule-'+i).outerHeight() + 10;
        };


        $('.video-player-title-text').html(title);
        $('#block-schedule-list-video').scrollTop(distance);

    }

    autoScrollAudio();
    autoScrollVideo();

    // setInterval(autoScrollAudio, 1000);
