    <script>
        $(function() {
            $('.comment-button').click(function(event) {
                $('.parent-comment').val($(this).attr('data-parent'));
            });

            $('.sendComment').click(function(event) {
                $('.parent-comment').val();
                var content = $('#comment-box').val();
                var slug = 'nam-dong-trung-ha-thao-san-pham-huu-co-cua-lao-cai';
                var parent = $('.parent-comment').val();
                var token = $('input[name="_token"]').val();
                var fullname = $('input[name="fullname"]').val();
                var email = $('input[name="email"]').val();

                if(!validateEmail(email) || $('#comment-fullname').val().length === 0 ){
                    if($('#comment-fullname').val().length === 0 ){
                        $('#comment-fullname').addClass('error');
                    }else{
                        $('#comment-fullname').removeClass('error');
                    }
                    if(!validateEmail(email)){
                        $('#comment-email').addClass('error');
                    }else{
                        $('#comment-email').removeClass('error');
                    }
                    return false;
                }

                $.ajax({
                    url: 'http://laocaitv.vn/ajax/send-comment',
                    type: 'POST',
                    data: {content: content, _token: token, slug: slug, parent: parent, fullname: fullname, email: email},
                    success: function(res){
                        $('#comment-box').val('');
                        $('#modal-comment').modal('hide');
                    }
                })

            });
        });
        function validateEmail(sEmail) {
            var filter = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
            if (filter.test(sEmail)) {
                return true;
            }
            else {
                return false;
            }
        }
